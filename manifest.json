{
  "manifest_version": 2,

  "background":{
    "scripts":["js/jquery.min.js", "bg.js", "ui.js"]
  },

  "name": "Inject Cookies",
  "description": "This will inject cookies for the next URL.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "background",
    "webRequest",
    "storage",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
